{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch src/server.ts",
    "dev:ts-node": "ts-node-dev --respawn --esm src/server.ts",
    "build": "tsc -b && vite build",
    "build:check": "tsc -b && vite build --mode production",
    "build:ts": "tsc -b",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "pre-commit": "lint-staged && yarn build:ts",
    "pre-push": "yarn build:check",
    "validate": "yarn format && yarn lint && yarn build:check",
    "validate:fix": "yarn format:write && yarn lint:fix && yarn build:check",
    "start": "node dist/index.js",
    "server": "nodemon src/server.ts",
    "create-admin": "tsx src/utils/createDummyAdmin.ts",
    "create-dummy-admin": "tsx src/utils/createDummyAdmin.ts",
    "fix-user-roles": "tsx src/utils/fixUserRoles.ts",
    "test:api": "tsx src/utils/testAPIs.ts",
    "test:s3": "tsx src/utils/testS3.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.995.0",
    "@emailjs/browser": "^4.4.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.2",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "react-router": "^7.4.1",
    "react-router-dom": "^7.13.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.21.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.9.2",
